                        },
                        "description": {
                            "type": "string"
                        },
                        "downloadUrl": {
                            "type": "string",
                            "format": "uri",
                            "example": "https://kf.kobotoolbox.org/forms/6/form.xml"
                        },
                        "manifestUrl": {
                            "type": "string",
                            "format": "uri",
                            "example": "https://kf.kobotoolbox.org/xformsManifest/6"
                        }
                    },
                    "xml": {
